{
  "name": "sistema-dideco",
  "private": true,
  "version": "1.0.4",
  "type": "module",
  "main": "electron/main.js",
  "author": "Municipalidad San Pedro",
  "description": "Sistema de Gesti√≥n DIDECO",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "electron:dev": "concurrently -k \"cross-env BROWSER=none npm run dev\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "dist": "npm run build && electron-builder"
  },
"build": {
    "appId": "com.dideco.app",
    "productName": "Sistema DIDECO",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "server/**/*",
      "package.json"
    ],
    "asar": false,
    "win": {
      "target": "nsis",
      "icon": "public/icon.ico",
      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
